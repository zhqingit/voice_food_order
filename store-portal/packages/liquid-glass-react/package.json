{
  "name": "@zhqingit/liquid-glass-react",
  "version": "0.1.0",
  "private": false,
  "description": "Liquid glass UI primitives for React.",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
  "registry": "https://npm.pkg.github.com"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup && node scripts/copy-css.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.6.0"
  }
}
